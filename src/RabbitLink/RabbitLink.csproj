<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Authors>Artur Kraev</Authors>
    <Description>Advanced .Net API for RabbitMQ</Description>
    <Copyright>Artur Kraev 2015-2017</Copyright>
    <PackageProjectUrl>https://github.com/rabbit-link/rabbit-link</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/rabbit-link/rabbit-link/master/LICENSE</PackageLicenseUrl>
    <PackageTags>RabbitMQ Messaging AMQP</PackageTags>
    <Version>0.4.1</Version>
    <PackageReleaseNotes>* Changed dependency tree.
* Removed loggers
</PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Consumer\LinkConsumer.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Consumer\LinkConsumer.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.3.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="RabbitMQ.Client" Version="5.0.1" />
  </ItemGroup>
</Project>